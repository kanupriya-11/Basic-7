#include <iostream>
using namespace std;

int main() {
   int n,count=0;
   cout<<"Enter any number: ";
   cin>>n;
   
   for(int i=1;i<n+1;i++)
    {
        if(n%i == 0)
          count++;
    }
    
    if(count>2)
    {
        cout<<"Not Prime";
    }
    else if(n==0 || n==1)
    {
        cout<<"Neither Prime nor Cpmposite";
    }
    else
    {
        cout<<"Is Prime";
    }
    return 0;
}
